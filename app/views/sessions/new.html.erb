<body class="homepage sixteen columns">
  <div class="wrapper-new">
    <div class="login nine columns">
    <h1 class="login-text">Login</h1>

      <%= form_tag sessions_path do %>
        <%= label_tag :email %>
        <%= text_field_tag :email %>

        <%= label_tag :password %>
        <%= password_field_tag :password %>

        <%= submit_tag :login %>

      <% end %>
    </div>
  </div>
</body>
